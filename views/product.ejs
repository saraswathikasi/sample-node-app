       .flex-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between; /* Adjust as needed */
}

.item {
  position: relative;
  flex-basis: calc(33.33% - 20px); /* Adjust as needed */
  margin-bottom: 20px; /* Adjust as needed */
}

.item:nth-child(3n+1) {
  flex-basis: calc(66.66% - 20px);
}

.item::before {
  content: "";
  display: block;
  padding-top: 100%; /* Aspect ratio (e.g., 1:1 for square) */
}

.item img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover; /* Ensure the image covers the box */
  border: 3px solid #000; /* Add border for the box */
}
.a{
    display: flex;
  justify-content:space-between;
  flex-wrap: wrap;
  flex-direction: row;
   margin-bottom: 50px;
   

}

